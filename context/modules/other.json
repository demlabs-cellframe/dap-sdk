{
  "module": "other",
  "description": "Additional components and utilities",
  "components": {
    "io": {
      "path": "io/",
      "description": "Input/output operations",
      "includes": "io/include/",
      "platform_support": ["linux"],
      "tests": "io/test/"
    },
    "global_db": {
      "path": "global-db/",
      "description": "Global database functionality",
      "includes": "global-db/include/",
      "tests": "global-db/test/"
    },
    "plugin": {
      "path": "plugin/",
      "description": "Plugin system",
      "includes": "plugin/include/",
      "source": "plugin/src/"
    },
    "avrestream": {
      "path": "avrestream/",
      "description": "AVR streaming functionality",
      "includes": "avrestream/include/"
    },
    "examples": {
      "path": "examples/",
      "description": "Example code and tutorials"
    },
    "test_framework": {
      "path": "test-framework/",
      "description": "Testing framework and utilities",
      "documentation": "test-framework/docs/"
    },
    "third_party": {
      "path": "3rdparty/",
      "description": "Third-party dependencies",
      "libraries": {
        "json_c": "3rdparty/json-c/",
        "libmdbx": "3rdparty/libmdbx/",
        "rpmalloc": "3rdparty/rpmalloc/",
        "secp256k1": "3rdparty/secp256k1/", 
        "shishua": "3rdparty/shishua/",
        "uthash": "3rdparty/uthash/",
        "wepoll": "3rdparty/wepoll/"
      }
    }
  },
  "build_system": {
    "root_cmake": "CMakeLists.txt",
    "cmake_modules": "cmake/",
    "project_config": "project.yaml",
    "ci_cd": ".gitlab-ci.yml"
  }
} 